<Window x:Class="Microprocessor8085Simulator.SetStartAdd"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="SetStartAdd" Height="220" Width="350" ResizeMode="CanResizeWithGrip" ShowInTaskbar="False" WindowStartupLocation="CenterOwner" xmlns:my="clr-namespace:Microprocessor8085Simulator">
    <Grid Background="{StaticResource GPB}">
        <Label Content="Set Starting Address (PC)" FontSize="20" Height="40" HorizontalAlignment="Left" Margin="50,21,0,0" Name="label2" VerticalAlignment="Top" />
        <my:myTextBox4d FontWeight="Bold" FontSize="18" Height="30" HorizontalAlignment="Left" Margin="116,78,0,0" x:Name="newPC" VerticalAlignment="Top" Width="88" />
        <Button Content="OK" Height="23" HorizontalAlignment="Left" IsDefault="True" Margin="50,132,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" />
        <Button Content="Cancel" Height="23" HorizontalAlignment="Left" Margin="205,132,0,0" Name="button2" VerticalAlignment="Top" Width="75" IsCancel="True" />
    </Grid>
</Window>

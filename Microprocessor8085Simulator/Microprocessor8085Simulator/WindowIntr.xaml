<Window x:Class="Microprocessor8085Simulator.WindowIntr"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="INTR" Height="175" Width="300" xmlns:my="clr-namespace:Microprocessor8085Simulator"
        ResizeMode="CanResizeWithGrip" ShowInTaskbar="False" WindowStartupLocation="CenterOwner">
    <Grid Background="AliceBlue">
        <my:myTextBox4d Height="23" HorizontalAlignment="Left" Margin="77,55,0,0" x:Name="tbAdd" VerticalAlignment="Top" Width="120" />
        <Label FontSize="16" Content="Enter the address to branch" Height="28" HorizontalAlignment="Left" Margin="36,16,0,0" Name="label1" VerticalAlignment="Top" />
        <Button Content="OK" IsDefault="True" Height="23" HorizontalAlignment="Left" Margin="36,96,0,0" Name="button1" VerticalAlignment="Top" Width="75" Click="button1_Click" />
        <Button Content="CANCEL" IsCancel="True" Height="23" HorizontalAlignment="Left" Margin="160,96,0,0" Name="button2" VerticalAlignment="Top" Width="75" />
    </Grid>
</Window>
